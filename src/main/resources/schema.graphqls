type Link {
    id: ID!
    url: String!
    description: String!
}

type Person {
	name: String!
	born: Int
}

type Query {
    allLinks: [Link]
    allPersons: [Person]
}

type Mutation {
    createLink(url: String!, description: String!): Link
}

schema {
    query: Query
    mutation: Mutation
}
